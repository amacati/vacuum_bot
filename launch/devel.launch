<?xml version="1.0" ?>
<launch>

  <!-- doc Launches ObjectSyncer, TFPub and CameraGuiding for basic functionality. Enables execution of single actions over the terminal. -->

  <node pkg="vacuum_bot" name="object_grabber" type="ObjectGrabber.py" output="screen"/>

  <node pkg="vacuum_bot" name="tf_pub" type="TFPub.py" output="screen"/>

  <node pkg="vacuum_bot" name="camera_guiding" type="CameraGuiding.py" output="screen"/>

  <node pkg="vacuum_bot" name="devel" type="devel.py" output="screen"/>

</launch>
